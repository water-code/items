/* Searcher/searchList/searchList.wxss */
.van-search {
  position: fixed;
  width: 100%;
  top: 0;
  z-index: 1;
}
.pull-list {
  margin-top: 108rpx;
}
.pull-list view {
  padding: 20rpx 0;
  margin: 0 20rpx;
  border-bottom: 1px solid #efefef;
  font-size: 28rpx;
  overflow: hidden; /* 超出部分隐藏 */
  white-space: nowrap; /* 不换行 */
  text-overflow: ellipsis; /* 文字超出显示省略号 */
}

.history {
  margin-top: 150rpx;
}
.top {
  display: flex;
  justify-content: space-between;
}
.content {
  display: flex;
  /* 超出盒子宽度换行 */
  flex-wrap: wrap;
}
.content view {
  margin: 5px 10px;
  background-color: #f2f3f5;
  border-radius: 10px;
  padding: 5px;
}
.van-list {
  margin: 100px 0 50px 0;
}